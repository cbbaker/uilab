{
    "linkRow": {
        "type": "function",
        "arguments": ["text", "url"],
        "body": {
            "type": "row",
            "children": {
                "link": {
                    "type": "link",
                    "text": "text",
                    "classes": [],
                    "url": "url"
                }
            }
        }
    },
    "main": {
        "type": "panel",
        "children": {
            "heading": {
                "type": "title",
                "text": "UILab",
                "level": 3,
                "classes": ["panel-title"]
            },
            "body": {
                "type": "container",
                "children": {
                    "showList": {
                        "type": "linkRow",
                        "arguments": {
                            "text": "List rides",
                            "url": "/index/html#showList.json"
                        }
                    },
                    "editList": {
                        "type": "linkRow",
                        "arguments": {
                            "text": "List rides with edit",
                            "url": "/index/html#editList.json"
                        }
                    }
                }
            }
        }
    }
}
